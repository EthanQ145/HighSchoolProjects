#kivy: 2.1.0


<StartScreen>
    label: label
    ready_input: ready_input
    start: start
    notice: notice
    Image:
        source: "unnamed.png"
        allow_stretch: True
        keep_ratio: False
        FloatLayout:
            size: root.size
            Label:
                id: title
                text: 'IMPORTANT INFO'
                color: 0, 0, 0.4, 1
                background_color: 0, 0, 0, 0
                font_size: self.parent.width * 0.05
                size_hint: None, None
                size: self.parent.width * 0.5, self.parent.height * 0.1
                text_size: self.width, self.height
                center_x: self.parent.width * 0.55
                top: self.parent.height
            Label:
                id: label
                text: ''
                background_color: 0, 0, 0, 0
                color: 0, 0, 0.502, 1
                font_size: self.parent.width * 0.018
                size_hint: None, None
                size: self.parent.width * 0.85, self.parent.height * 0.8
                text_size: self.width, self.height
                center_x: self.parent.width * 0.5
                y: self.parent.height * 0.2
            TextInput:
                id: ready_input
                multiline: False
                size_hint: None, None
                size: self.parent.width * 0.4, self.parent.height * 0.05
                center_x: self.parent.width * 0.5
                y: self.parent.height * 0.1
            Button:
                id: start
                text: "SUBMIT"
                bold: True
                font_size: self.parent.width * 0.03
                on_press: root.start_game()
                background_color: 0, 0, 0, 0
                color: 0, 0, 0.502, 1
                size_hint: None, None
                size: self.parent.width * 0.3, self.parent.height * 0.08
                right: self.parent.width
                y: 0
            Notice:
                id: notice


<Welcome>
    start: start
    Label:
        background_color: 0, 1, 1, 1
        allow_stretch: True
        keep_ratio: False
        FloatLayout:
            size: self.parent.size
            Label:
                text: 'Cultural Hackathon'
                color: 0, 1, 1, 1
                background_color: 0, 0, 0, 0
                font_size: self.parent.width * 0.05
                size_hint: None, None
                size: self.parent.width * 0.5, self.parent.height * 0.2
                center_x: self.parent.width * 0.5
                top: self.parent.height * 0.85
            Button:
                id: start
                text: 'START'
                background_color: 0, 0, 0, 0
                font_size: self.parent.width * 0.02
                on_press: root.start_time()
                size_hint: None, None
                size: self.parent.width * 0.3, self.parent.height * 0.1
                center_x: self.parent.width * 0.5
                top: self.parent.height * 0.45



<Inventory>
    inventory_button: inventory_button
    inventory_background: inventory_background
    inventory_space: inventory_space
    Image:
        source: 'inventory_button.png'
        allow_stretch: True
        id: inventory_button
        size_hint: None, None
        pos_hint: None, None
        size: self.parent.width * 0.2, self.parent.height * 0.05
        center_x: self.parent.width/2
        y: 0
        Button:
            opacity: 0
            size: self.parent.size
            pos: self.parent.pos
            on_press: root.open_inventory()
    Image:
        id: inventory_background
        allow_stretch: True
        keep_ratio: False
        source: 'inventory.png'
        size_hint: None, None
        pos_hint: None, None
        size: self.parent.width, self.parent.height * 0.2
        pos: 0, -self.parent.height * 0.2
        StackLayout:
            id: inventory_space
            size: self.parent.size
            pos: self.parent.pos
            padding: self.parent.height * 0.15
            spacing: 7


<BackToMenuButton>
    Button:
        on_press: app.root.current = 'game'
        size_hint: None, None
        size: self.parent.width * 0.05, self.parent.width * 0.05
        x: self.parent.width * 0.02
        top: self.parent.height * 0.98
        text: "BACK"


<Notice>
    text: ''
    color: 1, 0, 0, 1
    font_size: self.parent.width*0.03
    size_hint: None, None
    x: self.parent.width * 0.05
    bottom: self.parent.height * 0.2
    size: self.texture_size
    text_size: self.parent.width * 0.35, None


